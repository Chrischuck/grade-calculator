{
  "presets": ["es2015", "stage-0", "react"],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  },
  "plugins": [
    "transform-flow-strip-types",
		"transform-runtime",
    "add-module-exports",
    "closure-elimination",
    "transform-react-constant-elements",
    "transform-react-remove-prop-types"
	]
}
